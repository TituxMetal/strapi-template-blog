{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "pageInfo": {
      "type": "component",
      "repeatable": false,
      "component": "page.page-info",
      "required": true
    }
  }
}
